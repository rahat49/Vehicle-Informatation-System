@model VIMS.DataModel.Renewapply

@{
    ViewData["Title"] = "Renew Application Details";
    Layout = "_AdminLayout";
}
<style>
    .container {
        margin: 10px;
    }

    .row {
        margin-bottom: 10px;
    }
</style>
<div class="container">

    <h1>Renew Application Request</h1>

    <div>
        <h4>Details</h4>
        <hr />
        <dl class="row">
           
            <dt class="col-sm-4">
                Vehicle Owner Name
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.OwnerName)
            </dd>
            <dt class="col-sm-4">
                Owner Address
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.OwnerAddress)
            </dd>
            <dt class="col-sm-4">
                Phone Number
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.Phone)
            </dd>
            <dt class="col-sm-4">
                Vehicle Category
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.Vcategory)
            </dd>
            <dt class="col-sm-4">
                Vehicle Number
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.Vehnum)
            </dd>
            <dt class="col-sm-4">
                Brand Name
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.Brandname)
            </dd>
            <dt class="col-sm-4">
                Color
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.Color)
            </dd>
            <dt class="col-sm-4">
                Brand Name
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.Brandname)
            </dd>
            <dt class="col-sm-4">
                Chessis No.
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.ChassisNo)
            </dd>
            <dt class="col-sm-4">
                Engine No.
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.EngineNo)
            </dd>
            <dt class="col-sm-4">
                Vehicle Registration Date
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.Registrationdate)
            </dd>
            <dt class="col-sm-4">
                Tax Token Number
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.TaxTNumber)
            </dd>
            <dt class="col-sm-4">
                Tax Token Expiry
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.TTExpirydate)
            </dd>
            <dt class="col-sm-4">
                Fees
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.Fees) Taka
            </dd>
            <dt class="col-sm-4">
                Status
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.Status)
            </dd>
            
        </dl>
    </div>
    <div>

        <a asp-action="RenewApproved" class="text-decoration-none">Back to List</a> |
        <a asp-action="AdminDashboard"> <i class='fa fa-home' style="font-size:20px;color:black"></i></a>
    </div>
</div>

